{
  "name": "@quantinuum/quantinuum-ui",
  "version": "1.0.0",
  "description": "Quantinuum design tokens and React components",
  "private": false,
  "author": {
    "name": "Aidan Keay",
    "email": "aidan.keay@quantinuum.com"
  },
  "sideEffects": false,
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "tsc",
    "build": "rm -rf ./dist && rollup -c"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "remove-unused-components",
        "prerelease": true
      },
    {
        "name": "fix-move-deps",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "tag": "latest",
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/src/index.js",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/types/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./tokens.css": "./dist/tokens.css"
  },
  "types": "./dist/types/src/index.d.ts",
  "mode": "production",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CQCL/quantinuum-ui.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CQCL/quantinuum-ui/issues"
  },
  "homepage": "https://github.com/CQCL/quantinuum-ui#readme",
